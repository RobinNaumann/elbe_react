{
  "name": "elbe-ui",
  "version": "0.2.5",
  "author": "Robin Naumann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinNaumann/moewe_ts.git"
  },
  "keywords": [
    "ui",
    "ui-framework",
    "flutter"
  ],
  "files": [
    "elbe.scss",
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -r ./dist ; bun build --target=node src/index.tsx --outdir=dist && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json"
  },
  "module": "src/index.tsx",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.438.0",
    "preact": "^10.23.2"
  }
}
