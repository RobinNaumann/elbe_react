{
  "name": "elbe-ui",
  "version": "2.0.11",
  "author": "Robin Naumann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinNaumann/elbe_react.git"
  },
  "keywords": [
    "ui",
    "ui-framework",
    "flutter",
    "react"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:ts": "bun i && tsc",
    "build:dts": "tsc --declaration",
    "build:css": "sass -q style/elbe-util.scss dist/elbe.css",
    "build": "rm -rf ./dist && bun run build:ts && bun run build:dts && bun run build:css ",
    "dev": "bun run build && (cd example && bun run dev)",
    "pub": "npm login && bun run build && npm publish && bun run pub:example",
    "pub:example": "bun run build && (cd example && bun run build) && open example/dist"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "sass": "^1.97.0"
  },
  "peerDependencies": {
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "dependencies": {
    "colors-convert": "^1.4.1",
    "lucide-react": "^0.561.0",
    "react-markdown": "^10.1.0",
    "wouter": "^3.8.1"
  }
}